<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>

    <!-- Responsive -->
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />

    <!-- Bootstrap -->
    <link href="@Url.Content("~/Content/css/bootstrap.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/css/bootstrap-theme.min.css")" rel="stylesheet" />

    <!-- CSS -->
    <link href="@Url.Content("~/Content/css/layout/layout.css")" rel="stylesheet" />

    @RenderSection("Style", false)
</head>
<body>
    @RenderBody()
</body>
</html>

<!-- JQuery-->
<script src="@Url.Content("~/Scripts/jquery-3.2.0.min.js")"></script>
<script src="@Url.Content("~/Scripts/jquery.mask.js")"></script>
<script src="@Url.Content("~/Scripts/jquery.blockUI.js")"></script>

<!-- Bootstrap -->
<script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>

<!-- SweetAlert -->
<script src="@Url.Content("~/Scripts/sweetalert.min.js")"></script>

<!-- SweetAlert Success -->
@if (TempData["Success"] != null)
{
    <script>
        swal({
            title: "Sucesso!",
            text: "@TempData["Success"]",
            icon: "success",
            button: "OK"
        });
    </script>
}

<!-- SweetAlert Error -->
@if (TempData["Error"] != null)
{
    <script>
        swal({
            title: "Erro!",
            text: "@TempData["Error"]",
            icon: "error",
            button: "OK"
        });
    </script>
}

@RenderSection("Script", false)